{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": [
                "ec2:DetachVolume",
                "ec2:AttachVolume",
                "ec2:RebootInstances",
                "ec2:TerminateInstances",
                "iam:GetInstanceProfile",
                "ec2:CreateKeyPair",
                "ec2:CreateTags",
                "ec2:CreateImage",
                "ec2:RunInstances",
                "ec2:AuthorizeSecurityGroup*",
                "ec2:StopInstances",
                "iam:ListInstanceProfiles",
                "ec2:Describe*",
                "ec2:GetPasswordData",
                "ec2:RevokeSecurityGroup*",
                "ec2:StartInstances",
                "ec2:CreateSecurityGroup",
                "ec2:ModifyInstanceAttribute",
                "ec2:DeleteKeyPair"
            ],
            "Resource": "*"
        },
        {
            "Sid": "VisualEditor1",
            "Effect": "Allow",
            "Action": "iam:PassRole",
            "Resource": "arn:aws:iam::182402351974:role/us-east-1-inflight_packer_run"
        }
    ]
}
